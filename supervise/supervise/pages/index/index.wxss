.list{
    display: flex;
    flex-wrap: wrap;
}
.list_item{
    display: block;
    width: 33.333%;
    height: 270rpx;
    border-bottom: 1px solid rgba(221, 220, 220, 0.822);
    border-right: 1px solid rgba(221, 220, 220, 0.822);
    box-sizing: border-box;
    padding-top: 80rpx; 
}
.list_icon{
    width: 100rpx;
    height: 100rpx;
    margin: 0 auto;
}
.list_icon image{
    width: 100%;
    height: 100%;
}
.list_title{
    text-align: center;
    font-size:26rpx;
    margin-top: 10rpx;
}
